<div class="tab-pane fade show active" id="overview">
  <div class="row">
    <div class="col-xxl-4 col-md-6">
      <div class="card info-card sales-card">
        <div class="card-body">
          <h5 class="card-title">Total Number Of Issued Cards</h5>

          <div class="d-flex align-items-center">
            <div class="ps-3">
              <h6 id="issuedCardCount">@Model.issuedCardCount</h6>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="col-xxl-4 col-md-6">
        <div class="card info-card">
            <div class="card-body">
            <h5 class="card-title">Total Number Of Name Modification Requests</h5>

            <div class="d-flex align-items-center">
                <div class="ps-3">
                <h6 id="NameModifyRequestsCount">@Model.nameModifyCount</h6>
                </div>
            </div>
            </div>
        </div>
    </div>

    <div class="col-xxl-4 col-xl-12">
      <div class="card info-card customers-card">
        <div class="card-body">
          <h5 class="card-title">Total Number Of Image Update Requests</h5>

          <div class="d-flex align-items-center">
            <div class="ps-3">
              <h6 id="ImageUpdateRequestsCount">@Model.imageUpdateCount</h6>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
